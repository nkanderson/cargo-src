{
  "globals": {},
  "env": {
    "browser": true,
    "jquery": true
  },
  "rules": {
    "react/prop-types": 0
  },
  "plugins": [
    "react",
    "jquery"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
    }
  },
  "parser": "espree"
}
